{
  "name": "@hemantanshu/nestjs-test-package",
  "version": "1.0.0",
  "description": "NestJS npm package starter",
  "author": "Hemant Kumar Sah <hemantanshu@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjsplus/nestjs-package-starter"
  },
  "bugs": "https://github.com/nestjsplus/nestjs-package-starter",
  "peerDependencies": {
    "@nestjs/common": "^6.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.11",
    "@nestjs/core": "^7.0.11",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.11",
    "@nestjs/swagger": "^4.5.6",
    "@nestjs/typeorm": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/lodash": "^4.14.149",
    "bcrypt": "^4.0.1",
    "cache-manager": "^3.2.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "commander": "^5.1.0",
    "config": "^3.3.1",
    "express-http-context": "^1.2.3",
    "express-session": "^1.17.0",
    "moment": "^2.24.0",
    "mysql": "^2.18.1",
    "nestjs-console": "^3.0.5",
    "parse-bearer-token": "^1.0.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "session-file-store": "^1.4.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24",
    "typeorm-naming-strategies": "^1.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.11",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.13.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
